eturn useer;\n  }\n}\n\n\nconst getters = {\n  getUsers(state){\n    return state.users;\n  },\n  getUserById2: (state) => (id) => {\n    console.log('the User ');\n    const actifUser = state.users[0].find(user => user.id == id);\n    console.log(actifUser);\n    return actifUser;\n  }\n}\n\nconst actions = {\n  setUsers(context,payload){\n    context.commit('setUsers',payload);\n  },\n  getUserById(context,payload){\n    context.commit('getUserById',payload);\n  }\n}\n\nexport default new Vuex.Store({\n  state,\n  mutations,\n  actions,\n  getters\n})"]}]}