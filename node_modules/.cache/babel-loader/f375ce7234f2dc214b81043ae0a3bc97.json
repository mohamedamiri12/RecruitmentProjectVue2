,CAAA,UAAA,QAAA,EAAA;AACA,eAAA,QAAA,CAAA,WAAA,CAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CAAA;AACA,OAFA,CAAA;AAGA,KAVA;AAWA,IAAA,IAXA,kBAWA;AACA,aAAA,KAAA,SAAA,CAAA,MAAA;AACA;AAbA,GApBA;AAmCA,EAAA,KAAA,EAAA;AACA,IAAA,MAAA,EAAA;AACA,MAAA,SAAA,EAAA,IADA;AAEA,MAAA,OAFA,mBAEA,KAFA,EAEA;AACA,YAAA,KAAA,CAAA,KAAA,IAAA,KAAA,CAAA,KAAA,CAAA,IAAA,EAAA;AACA,eAAA,WAAA,GAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA;AACA;AANA;AADA,GAnCA;AA6CA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,oBACA,MADA,EACA;AACA,cAAA,MAAA;AACA,aAAA,QAAA;AACA,iBAAA,SAAA;;AACA,aAAA,UAAA;AACA,iBAAA,WAAA;;AACA,aAAA,SAAA;AACA,iBAAA,SAAA;;AACA,aAAA,QAAA;AACA,iBAAA,QAAA;;AACA;AACA;AAVA;AAYA,KAdA;AAeA,IAAA,aAfA,2BAeA;AAAA;;AACA,WAAA,aAAA,GAAA,IAAA;AACA,WAAA,MAAA,CACA,QADA,CACA,eADA,EAEA,IAFA,CAEA,UAAA,MAAA,EAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,OAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,wCAAA;AACA,OATA;AAUA,KA3BA;AA4BA,IAAA,cA5BA,0BA4BA,EA5BA,EA4BA;AAAA;;AACA,WAAA,MAAA,CACA,QADA,CACA,gBADA,EACA,EADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,sCAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,qCAAA;;AACA,QAAA,MAAA,CAAA,aAAA;AACA,OANA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,OAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,2BAAA;AACA,OAVA;AAWA,KAxCA;AAyCA,IAAA,YAzCA,wBAyCA,MAzCA,EAyCA;AACA,UAAA,EAAA,GAAA,MAAA,CAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,sBAAA,EAAA;AAAA,OAAA;AACA;AA5CA,GA7CA;AA2FA,EAAA,OA3FA,qBA2FA;AACA,SAAA,aAAA;AACA;AA7FA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container-fluid\">\r\n    <div class=\"card\">\r\n      <div class=\"card-header d-flex justify-content-between\">\r\n        <b class=\"d-block\">Contracts</b>\r\n        <div class=\"box\">\r\n          <div class=\"input-group\">\r\n            <div class=\"form-outline\">\r\n              <input\r\n                id=\"search-input\"\r\n                type=\"search\"\r\n                class=\"form-control\"\r\n                placeholder=\"Search\"\r\n                v-model=\"search\"\r\n              />\r\n            </div>\r\n            <button id=\"search-button\" type=\"button\" class=\"btn btn-primary\">\r\n              <i class=\"fas fa-search\"></i>\r\n            </button>\r\n          </div>\r\n          \r\n        </div>\r\n      </div>\r\n      <div class=\"card-body\">\r\n        <!-- Loading Spinner -->\r\n        <div\r\n          class=\"d-flex justify-content-center px-4 py-3 my-3\"\r\n          v-if=\"isLoading\"\r\n        >\r\n          <b-spinner\r\n            variant=\"success\"\r\n            label=\"Spinning\"\r\n            class=\"mr-3\"\r\n          ></b-spinner>\r\n          Loading ...\r\n        </div>\r\n        <!-- Contracts Table for computer Screens -->\r\n        <b-table\r\n          v-if=\"contracts.length\"\r\n          head-variant=\"dark\"\r\n          hover\r\n          striped\r\n          id=\"contracts-table\"\r\n          :items=\"contracts\"\r\n          :fields=\"fields\"\r\n          :per-page=\"perPage\"\r\n          :total-rows=\"rows\"\r\n          :current-page=\"currentPage\"\r\n          responsive\r\n          @row-clicked=\"showContract\"\r\n          class=\"text-center\"\r\n        >\r\n          <!-- Client Button -->\r\n          <template #cell(client)=\"row\">\r\n            <div class=\"\">\r\n              <img\r\n                :src=\"\r\n                  'http://localhost:8000/storage/clientImages/' +\r\n                    row.item.id +\r\n                    '/' +\r\n                    row.item.client.client_image\r\n                \"\r\n                alt=\"\"\r\n                class=\"d-inline-block img-fluid img-client mx-3\"\r\n              />\r\n              <p class=\"d-inline-block text-center client-name\">\r\n                {{\r\n                  row.item.client.first_name + \" \" + row.item.client.last_name\r\n                }}\r\n              </p>\r\n            </div>\r\n          </template>\r\n          <!-- Candidate Button -->\r\n          <template #cell(candidate)=\"row\">\r\n            <div class=\"\">\r\n              <img\r\n                :src=\"\r\n                  'http://localhost:8000/storage/candidateImages/' +\r\n                    row.item.id +\r\n                    '/' +\r\n                    row.item.candidate.candidate_image\r\n                \"\r\n                alt=\"\"\r\n                class=\"d-inline-block img-fluid img-client mx-3\"\r\n              />\r\n              <p class=\"d-inline-block text-center client-name\">\r\n                {{\r\n                  row.item.candidate.first_name +\r\n                    \" \" +\r\n                    row.item.candidate.last_name\r\n                }}\r\n              </p>\r\n            </div>\r\n          </template>\r\n          <!-- Actions Button -->\r\n          <template #cell(actions)=\"row\">\r\n            <b-button\r\n              size=\"sm\"\r\n              variant=\"success mr-2\"\r\n              @click=\"validateContract(row.item.id)\"\r\n              class=\"mr-2\"\r\n              ><i class=\"fas fa-search mr-2\"></i> Validate</b-button\r\n            >\r\n\r\n            <b-button\r\n              size=\"sm\"\r\n              variant=\"danger mr-2\"\r\n              @click=\"deleteContract(row.item.id)\"\r\n              >Delete</b-button\r\n            >\r\n          </template>\r\n        </b-table>\r\n\r\n        <!-- contracts Table for mobile Screens -->\r\n\r\n        <div class=\"text-center no-items-box py-5\" v-if=\"!contracts.length\">\r\n          <i class=\"fas fa-ban\" style=\"color:red\"></i> No items to show\r\n        </div>\r\n      </div>\r\n      <!-- Loading Data Spinner -->\r\n      <div\r\n        class=\"d-flex justify-content-center px-4 py-3 my-3\"\r\n        v-if=\"isLoadingData\"\r\n      >\r\n        <b-spinner variant=\"primary\" label=\"Spinning\" class=\"mr-3\"></b-spinner>\r\n        Loading ...\r\n      </div>\r\n      <div class=\"card-footer text-muted w-100\">\r\n        <b-pagination\r\n          class=\"d-flex justify-content-center mt-2 mb-1\"\r\n          v-model=\"currentPage\"\r\n          :total-rows=\"rows\"\r\n          :per-page=\"perPage\"\r\n          aria-controls=\"contracts-table\"\r\n          first-text=\"⏮\"\r\n          prev-text=\"⏪\"\r\n          next-text=\"⏩\"\r\n          last-text=\"⏭\"\r\n        ></b-pagination>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Contracts\",\r\n  data() {\r\n    return {\r\n      fields: [\r\n        { key: \"client\", label: \"client\" },\r\n        { key: \"candidate\", label: \"candidate\" },\r\n        { key: \"start-date\", label: \"Start Date\" },\r\n        { key: \"actions\", label: \"Actions\" },\r\n      ],\r\n      errors: [],\r\n      loadingStatus: false,\r\n      error: \"\",\r\n      currentPage: 1,\r\n      perPage: 5,\r\n      isLoading: false,\r\n      isLoadingData: false,\r\n      search: null,\r\n    };\r\n  },\r\n  computed: {\r\n    /**\r\n     *  Return Users and Apply search filter\r\n     */\r\n    contracts() {\r\n      let contracts = this.$store.getters.getContracts;\r\n      if (this.search == null) return contracts;\r\n      return contracts.filter((contract) => {\r\n        return contract.description.match(this.search);\r\n      });\r\n    },\r\n    rows() {\r\n      return this.contracts.length;\r\n    },\r\n  },\r\n  watch: {\r\n    $route: {\r\n      immediate: true,\r\n      handler(route) {\r\n        if (route.query && route.query.page) {\r\n          this.currentPage = Number(route.query.page);\r\n        }\r\n      },\r\n    },\r\n  },\r\n  methods: {\r\n    getBadge(status) {\r\n      switch (status) {\r\n        case \"Active\":\r\n          return \"success\";\r\n        case \"Inactive\":\r\n          return \"secondary\";\r\n        case \"Pending\":\r\n          return \"warning\";\r\n        case \"Banned\":\r\n          return \"danger\";\r\n        default:\r\n          \"primary\";\r\n      }\r\n    },\r\n    loadContracts() {\r\n      this.isLoadingData = true;\r\n      this.$store\r\n        .dispatch(\"loadContracts\")\r\n        .then((result) => {\r\n          this.isLoadingData = false;\r\n        })\r\n        .catch((error) => {\r\n          this.isLoadingData = false;\r\n          console.log(error.message);\r\n          this.error = \"There was an error while bringing data\";\r\n        });\r\n    },\r\n    deleteContract(id) {\r\n      this.$store\r\n        .dispatch(\"deleteContract\", id)\r\n        .then((res) => {\r\n          this.success = \"Contracts's deleted successfully !!!\";\r\n          console.log(\"Contract's deleted successfully !!!\");\r\n          this.loadContracts();\r\n        })\r\n        .catch((error) => {\r\n          console.log(error.message);\r\n          this.error = \"The delete process failed\";\r\n        });\r\n    },\r\n    showContract(record) {\r\n      let id = record.id;\r\n      this.$router.push({ path: `contracts/${id}` });\r\n    },\r\n  },\r\n  created() {\r\n    this.loadContracts();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.img-client {\r\n  width: 40px;\r\n  height: 40px;\r\n  border-radius: 100%;\r\n}\r\n.client-name {\r\n  font-weight: 600;\r\n}\r\n</style>"],"sourceRoot":"src/views/contracts"}]}