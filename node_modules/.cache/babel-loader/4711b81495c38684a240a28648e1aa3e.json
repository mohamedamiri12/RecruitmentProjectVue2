ACA,aAAA,KAAA,MAAA,CAAA,MAAA;AACA;AAZA,GArBA;AAmCA,EAAA,KAAA,EAAA;AACA,IAAA,MAAA,EAAA;AACA,MAAA,SAAA,EAAA,IADA;AAEA,MAAA,OAFA,mBAEA,KAFA,EAEA;AACA,YAAA,KAAA,CAAA,KAAA,IAAA,KAAA,CAAA,KAAA,CAAA,IAAA,EAAA;AACA,eAAA,UAAA,GAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA;AACA;AANA;AADA,GAnCA;AA6CA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,GADA,EACA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,KAAA,EAAA;AAAA,UAAA,IAAA,EAAA;AAAA;AAAA,OAAA;AACA,KAHA;AAIA,IAAA,SAJA,qBAIA,EAJA,EAIA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,mBAAA,EAAA;AAAA,OAAA;AACA,KANA;AAOA,IAAA,UAPA,wBAOA;AAAA;;AACA,WAAA,aAAA,GAAA,IAAA;AACA,WAAA,MAAA,CACA,QADA,CACA,YADA,EAEA,IAFA,CAEA,UAAA,MAAA,EAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,OAPA;AAQA,KAjBA;AAkBA,IAAA,qBAlBA,mCAkBA;AACA,WAAA,MAAA,CACA,QADA,CACA,YADA,EAEA,IAFA,CAEA,UAAA,MAAA,EAAA,CAAA,CAFA,EAGA,KAHA,CAGA,UAAA,KAAA,EAAA,CAAA,CAHA;AAIA,KAvBA;AAwBA,IAAA,WAxBA,uBAwBA,EAxBA,EAwBA;AAAA;;AACA,WAAA,MAAA,CACA,QADA,CACA,aADA,EACA,EADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,kCAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,kCAAA;;AACA,QAAA,MAAA,CAAA,UAAA;AACA,OANA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,OAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,2BAAA;AACA,OAVA;AAWA,KApCA;AAqCA,IAAA,SArCA,qBAqCA,MArCA,EAqCA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,WAAA,eAAA,GAAA,MAAA;AACA;AAxCA,GA7CA;AAuFA,EAAA,OAvFA,qBAuFA;AACA,SAAA,UAAA;AACA;AAzFA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container-fluid\">\r\n    <div class=\"card\">\r\n      <div class=\"card-header d-flex justify-content-between\">\r\n        <b class=\"d-block\">Skills</b>\r\n        <div class=\"box\">\r\n          <div class=\"input-group\">\r\n            <div class=\"form-outline\">\r\n              <input\r\n                id=\"search-input\"\r\n                type=\"search\"\r\n                class=\"form-control\"\r\n                placeholder=\"Search\"\r\n                v-model=\"search\"\r\n              />\r\n            </div>\r\n            <button id=\"search-button\" type=\"button\" class=\"btn btn-primary\">\r\n              <i class=\"fas fa-search\"></i>\r\n            </button>\r\n          </div>\r\n          \r\n        </div></div>\r\n      <div class=\"card-body\">\r\n        <div\r\n          class=\"d-flex justify-content-center px-4 py-3 my-3\"\r\n          v-if=\"isLoading\"\r\n        >\r\n          <b-spinner\r\n            variant=\"success\"\r\n            label=\"Spinning\"\r\n            class=\"mr-3\"\r\n          ></b-spinner>\r\n          Loading ...\r\n        </div>\r\n        <!-- Users Table -->\r\n        <b-table\r\n          head-variant=\"dark\"\r\n          hover\r\n          striped\r\n          id=\"skills-table\"\r\n          :items=\"skills\"\r\n          :fields=\"fields\"\r\n          :per-page=\"perPage\"\r\n          :total-rows=\"rows\"\r\n          :current-page=\"currentPage\"\r\n          class=\"text-center\"\r\n        >\r\n          <!-- Actions Button -->\r\n          <template #cell(actions)=\"row\">\r\n            <b-button\r\n              size=\"sm\"\r\n              variant=\"primary mr-2\"\r\n              @click=\"showSkill(row.item.id)\"\r\n              class=\"mr-2\"\r\n              ><i class=\"fas fa-expand-alt mr-2\"></i>Show</b-button\r\n            >\r\n\r\n            <b-button\r\n              size=\"sm\"\r\n              variant=\"danger mr-2\"\r\n              @click=\"deleteSkill(row.item.id)\"\r\n              ><i class=\"fas fa-trash-alt mr-2\"></i>Delete</b-button\r\n            >\r\n          </template>\r\n        </b-table>\r\n      </div>\r\n      <!-- Loading Data Spinner -->\r\n      <div\r\n        class=\"d-flex justify-content-center px-4 py-3 my-3\"\r\n        v-if=\"isLoadingData\"\r\n      >\r\n        <b-spinner variant=\"primary\" label=\"Spinning\" class=\"mr-3\"></b-spinner>\r\n        Loading ...\r\n      </div>\r\n      <div class=\"card-footer text-muted w-100\">\r\n        <b-pagination\r\n          class=\"d-flex justify-content-center mt-2 mb-1\"\r\n          v-model=\"currentPage\"\r\n          :total-rows=\"rows\"\r\n          :per-page=\"perPage\"\r\n          aria-controls=\"skills-table\"\r\n          first-text=\"⏮\"\r\n          prev-text=\"⏪\"\r\n          next-text=\"⏩\"\r\n          last-text=\"⏭\"\r\n        ></b-pagination>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Skills\",\r\n  data() {\r\n    return {\r\n      showModalStatus: false,\r\n      fields: [\r\n        { key: \"name\", label: \"Name\" },\r\n        { key: \"actions\", label: \"Actions\" },\r\n      ],\r\n      isLoading: false,\r\n      currentPage: 1,\r\n      activePage: 1,\r\n      error: \"\",\r\n      success: \"\",\r\n      perPage: 5,\r\n      numberOfItems: 0,\r\n      headVariant: \"dark\",\r\n      isLoadingData: false,\r\n      search: null,\r\n    };\r\n  },\r\n  computed: {\r\n    skills() {\r\n      let skills = this.$store.getters.getSkills;\r\n      if (this.search == null) return skills;\r\n      return skills.filter((skill) => {\r\n        return (\r\n          skill.name.match(this.search) \r\n        );\r\n      });\r\n    },\r\n    rows() {\r\n      return this.skills.length;\r\n    },\r\n  },\r\n  watch: {\r\n    $route: {\r\n      immediate: true,\r\n      handler(route) {\r\n        if (route.query && route.query.page) {\r\n          this.activePage = Number(route.query.page);\r\n        }\r\n      },\r\n    },\r\n  },\r\n  methods: {\r\n    pageChange(val) {\r\n      this.$router.push({ query: { page: val } });\r\n    },\r\n    showSkill(id) {\r\n      this.$router.push({ path: `skills/${id}` });\r\n    },\r\n    loadSkills() {\r\n      this.isLoadingData = true;\r\n      this.$store\r\n        .dispatch(\"loadSkills\")\r\n        .then((result) => {\r\n          this.isLoadingData = false;\r\n        })\r\n        .catch((error) => {\r\n          this.isLoadingData = false;\r\n        });\r\n    },\r\n    loadSkillsAfterUpdate() {\r\n      this.$store\r\n        .dispatch(\"loadSkills\")\r\n        .then((result) => {})\r\n        .catch((error) => {});\r\n    },\r\n    deleteSkill(id) {\r\n      this.$store\r\n        .dispatch(\"deleteSkill\", id)\r\n        .then((res) => {\r\n          this.success = \"Skill's deleted successfully !!!\";\r\n          console.log(\"Skill's deleted successfully !!!\");\r\n          this.loadSkills();\r\n        })\r\n        .catch((error) => {\r\n          console.log(error.message);\r\n          this.error = \"The delete process failed\";\r\n        });\r\n    },\r\n    showModal(status) {\r\n      console.log(status);\r\n      this.showModalStatus = status;\r\n    },\r\n  },\r\n  created() {\r\n    this.loadSkills();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.table-thead div table thead {\r\n  background-color: #212529;\r\n  color: #fff;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/skills"}]}