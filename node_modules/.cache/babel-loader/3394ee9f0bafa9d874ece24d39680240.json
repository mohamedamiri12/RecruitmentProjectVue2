center px-4 py-3 my-3\"\r\n          v-if=\"isLoading\"\r\n        >\r\n          <b-spinner variant=\"success\" label=\"Spinning\"></b-spinner>\r\n        </div>\r\n        <form action=\"\" enctype=\"multipart/form-data\">\r\n          <CInput\r\n            description=\"Enter your first name\"\r\n            label=\"First Name\"\r\n            horizontal\r\n            v-model=\"candidate.first_name\"\r\n          />\r\n          <CInput\r\n            description=\"Enter your last name\"\r\n            label=\"Last Name\"\r\n            horizontal\r\n            v-model=\"candidate.last_name\"\r\n          />\r\n          <CInput\r\n            label=\"Phone\"\r\n            description=\"your phone number must be 10 caracters\"\r\n            placeholder=\"Enter your phone number\"\r\n            horizontal\r\n            v-model=\"candidate.phone_number\"\r\n          />\r\n          <CInput\r\n            label=\"Email\"\r\n            description=\"Please enter your email\"\r\n            placeholder=\"Enter your email\"\r\n            type=\"email\"\r\n            horizontal\r\n            autocomplete=\"email\"\r\n            v-model=\"candidate.email\"\r\n          />\r\n          <!-- Upload File -->\r\n          <p class=\"mt-3 mb-2\">Candidate Image</p>\r\n          <div class=\"custom-file\">\r\n            <input\r\n              type=\"file\"\r\n              class=\"custom-file-input\"\r\n              id=\"validatedCustomFile\"\r\n              required\r\n              accept=\"image/*\"\r\n              @change=\"uploadImage\"\r\n            />\r\n            <label class=\"custom-file-label\" for=\"validatedCustomFile\"\r\n              >Choose file...</label\r\n            >\r\n          </div>\r\n        </form>\r\n      </CCardBody>\r\n      <CCardFooter>\r\n        <CButton\r\n          type=\"submit\"\r\n          size=\"sm\"\r\n          color=\"primary\"\r\n          class=\"mr-3\"\r\n          @click=\"updateCandidate\"\r\n        >\r\n          <CIcon name=\"cil-check-circle\" /> Submit</CButton\r\n        >\r\n        <CButton type=\"reset\" size=\"sm\" color=\"danger\"\r\n          ><CIcon name=\"cil-ban\" /> Reset</CButton\r\n        >\r\n      </CCardFooter>\r\n    </CCard>\r\n  </CCol>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  props: [\"editedCandidate\"],\r\n  data() {\r\n    return {\r\n      /*firstName: this.editedClient.first_name,\r\n      lastName: this.client.last_name,\r\n      email: this.client.email,\r\n      password: this.client.password,\r\n      phoneNumber: this.client.phoneNumber,\r\n      status: this.client.status,*/\r\n      password: \"\",\r\n      isLoading: false,\r\n      successUpdate: \"\",\r\n      error: \"\",\r\n      candidate_image: null,\r\n      countDown: 5,\r\n    };\r\n  },\r\n  computed: {\r\n    candidate() {\r\n      return this.editedCandidate;\r\n    },\r\n    status() {\r\n      return this.candidate.is_suspended === 0 ? false : true;\r\n    },\r\n  },\r\n  methods: {\r\n    countDownTimer() {\r\n      if (this.countDown > 0) {\r\n        setTimeout(() => {\r\n          this.countDown -= 1;\r\n          this.countDownTimer();\r\n        }, 1000);\r\n      }\r\n    },\r\n    uploadImage(e) {\r\n      this.candidate_image = e.target.files[0];\r\n      console.log(this.candidate_image);\r\n    },\r\n    updateCandidate() {\r\n      this.isLoading = true;\r\n\r\n      const updatedCAndidate = new FormData();\r\n\r\n      updatedCAndidate.append(\"first_name\", this.candidate.first_name);\r\n      updatedCAndidate.append(\"last_name\", this.candidate.last_name);\r\n      updatedCAndidate.append(\"email\", this.candidate.email);\r\n      updatedCAndidate.append(\"password\", this.candidate);\r\n      updatedCAndidate.append(\"phone_number\", this.candidate.phone_number);\r\n      updatedCAndidate.append(\"candidate_image\", this.candidate_image);\r\n      updatedCAndidate.append(\"_method\", \"PUT\");\r\n\r\n      const id = this.candidate.id;\r\n\r\n      const apiUpdateRoute =\r\n        \"http://localhost:8000/api/administrator/candidates/\" + id;\r\n      const config = { headers: { \"content-type\": \"multipart/form-data\" } };\r\n\r\n      axios\r\n        .post(apiUpdateRoute, updatedCAndidate, config)\r\n        .then((res) => {\r\n          console.log(\"After Done Axios ==> update Candidate\");\r\n          this.isLoading = false;\r\n          this.successUpdate = \"The update succeded\";\r\n          this.countDownTimer();\r\n        })\r\n        .catch((error) => {\r\n          console.log(error.response.data);\r\n          this.isLoading = false;\r\n          this.error = \"Something wrong, Please try again\";\r\n          this.countDownTimer();\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/candidates"}]}